    .help{
        position: absolute;
        top: 10px;
        right: 10px;
        background: #fff;
        box-shadow: 0 0 10px rgba(0,0,0,0.1);
        display: flex;
        justify-content: center;
        align-items: center;
        flex-direction: column;
        width: 50px;
        height: 50px;
        border-radius: 50%;
        transition: all 0.5s ease;
        cursor: pointer;
        overflow: hidden;
    }
    .help:is(:not(:hover)) {
        animation: animat 2s infinite;
    }
    .help:hover{
        width: 300px;
        height: 100px;
        border-radius: 30px;
    }
    .help:hover .message{
        opacity: 1;
        transition: all 0.5s 0.5s ease;
    }
    .help:hover .userinfo{
        display: none;
    }
    .message{
        opacity: 0;
        display: flex;
        flex-direction: column;
        padding: 20px;
    }
    .message-title{
        font-size: 1.5em;
        margin-bottom: 10px;
        font-weight: bold;
        color: #333;
    }
    .userinfo{
        position: absolute;
        font-size: 1.5em;
        color: #333;
    }
